{
  "redirects": [
    {
      "source": "/volby/(.*)",
      "destination": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://volebnikalkulacka.cz/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://volebnikalkulacka.cz/api/:path*/"
    }
  ]
}
